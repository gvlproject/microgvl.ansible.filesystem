#%Module
# Built automatically by ansible

unset-env PYTHONPATH
unset-env LD_LIBRARY_PATH

prepend-path  PATH  {{ homebrew_path }}/.linuxbrew/bin
prepend-path  INFOPATH {{ homebrew_path }}/.linuxbrew/share/info
prepend-path  MANPATH  {{ homebrew_path }}/.linuxbrew/share/man

set-env HOMEBREW_TEMP="{{ gvl_tmp_path }}/homebrew_temp"
set-env HOMEBREW_CACHE="{{ gvl_tmp_path }}/homebrew_cache"
set-env KRAKEN_DEFAULT_DB="{{ homebrew_path }}/.linuxbrew/Cellar/kraken/krakendb"

module-whatis  Module automatically generated by GVL scripts, for linuxbrew system.
